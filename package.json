{
  "name": "@dariusbakunas/eve-esi-client",
  "version": "1.12.1",
  "description": "OpenAPI client for @dariusbakunas/eve-esi-client",
  "author": "OpenAPI-Generator Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/dariusbakunas/eve-esi-client.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "openapi-generator",
    "@dariusbakunas/eve-esi-client"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepare": "yarn build",
    "prettier": "prettier --write .",
    "commit": "cz"
  },
  "dependencies": {
    "axios": "^0.26.1"
  },
  "files": [
    "dist/*",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@types/node": "^12.11.5",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "release-please": "^14.17.0",
    "typescript": "^4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
