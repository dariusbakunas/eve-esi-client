{
  "name": "@dariusbakunas/eve-esi-client",
  "version": "1.12",
  "description": "OpenAPI client for @dariusbakunas/eve-esi-client",
  "author": "OpenAPI-Generator Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/dariusbakunas/eve-esi-client.git"
  },
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "openapi-generator",
    "@dariusbakunas/eve-esi-client"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepare": "yarn build",
    "prettier": "prettier --write .",
    "reset-hard": "git clean -dfx && git reset --hard && yarn",
    "version": "standard-version",
    "prepare-release": "run-s reset-hard version"
  },
  "dependencies": {
    "axios": "^0.26.1"
  },
  "files": [
    "dist/*",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^12.11.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.0"
  }
}
